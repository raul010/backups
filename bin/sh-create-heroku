#!/bin/bash
NC='\e[0m' # No Color
green='\e[0;32m'
light_green='\e[1;32m'
cyan='\e[0;36m'
light_blue='\e[1;34m'

echo -e " ${light_blue}
> Create Heroku Project ${NC}${cyan}
- Path: /home/raul/dev/works-py2/
- From: tests-django/
- To:   heroku-tests-django/tests-django${NC}
"
cd /home/raul/dev/works-py2/ && /usr/local/bin/grunt create;
cd -;
